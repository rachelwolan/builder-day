{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "builder-day",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-03-01",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": ".open-next/assets"
  },
  "observability": {
    "enabled": true
  },
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "my-bucket",
      "bucket_id": "123456789"
    }
  ]
}
